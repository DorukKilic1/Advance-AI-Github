     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "Distribution Statistics:\n",
      "Skewness: nan (0 = symmetric, >0 = right-skewed, <0 = left-skewed)\n",
      "Kurtosis: nan (0 = normal, >0 = heavy tails, <0 = light tails)\n"
     ]
    }
   ],
   "source": [
    "# Example: Create a histogram for a numeric column\n",
    "\n",
    "# Step 1: Select a numeric column to visualize\n",
    "# Replace 'column_name' with an actual column from your dataset\n",
    "target_col = df.select_dtypes(include=[np.number]).columns[0]\n",
    "print(f\"Visualizing column: {target_col}\")\n",
    "\n",
    "# Step 2: Create the histogram\n",
    "plt.figure(figsize=(10, 6))\n",
    "plt.hist(df[target_col], bins=50, edgecolor='black', alpha=0.7, color='steelblue')\n",
    "\n",
    "# Step 3: Add reference lines for mean and median\n",
    "plt.axvline(df[target_col].mean(), color='red', linestyle='--', \n",
    "            linewidth=2, label=f'Mean: {df[target_col].mean():.4f}')\n",
    "plt.axvline(df[target_col].median(), color='green', linestyle='--', \n",
    "            linewidth=2, label=f'Median: {df[target_col].median():.4f}')\n",
    "\n",
    "# Step 4: Labels and formatting\n",
    "plt.xlabel(target_col, fontsize=12)\n",
    "plt.ylabel('Frequency', fontsize=12)\n",
    "plt.title(f'Distribution of {target_col}', fontsize=14, fontweight='bold')\n",
    "plt.legend()\n",
    "plt.grid(True, alpha=0.3)\n",
    "plt.show()\n",
    "\n",
    "# Step 5: Print distribution statistics\n",
    "print(f\"\\nDistribution Statistics:\")\n",
    "print(f\"Skewness: {stats.skew(df[target_col]):.4f} (0 = symmetric, >0 = right-skewed, <0 = left-skewed)\")\n",
    "print(f\"Kurtosis: {stats.kurtosis(df[target_col]):.4f} (0 = normal, >0 = heavy tails, <0 = light tails)\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
